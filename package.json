{
  "name": "yas-modal",
  "version": "0.0.5",
  "scripts": {
    "dev": "vite dev",
	"build_doc": "vite build",
    "build": "svelte-kit sync && svelte-package",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@sveltejs/package": "^1.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte-check": "^3.0.1",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "peerDependencies": {
	"svelte": "^3.54.0"
  },
  "type": "module",
  "description": "Yet another svelte modal component. This one has a target.",
  "main": "svelte.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gigor/yas-modal.git"
  },
  "keywords": [
    "svelte",
    "modal",
    "tailwindcss"
  ],
  "author": "Igor Grechishkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gigor/yas-modal/issues"
  },
  "homepage": "https://github.com/gigor/yas-modal#readme"
}
